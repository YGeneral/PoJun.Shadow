{
  "AppSettings": {
    "SystemID": "PoJun.Shadow.API",
    "APIConfig": {
      "Port": "9999",
      "Environment": "localhost",
      "RequestSource": "http://*.baidu.com,http://127.0.0.1:9522" //设置跨域请求的来源
    },
    "DBConfig": {
      "MySql": {
        "PoJun_Shadow": "Server=127.0.0.1;Port=3306;Database=PoJun_Shadow;Uid=root;Pwd=123456;CharSet=utf8;",
        "PoJun_Shadow_Logs": "Server=127.0.0.1;Port=3306;Database=PoJun_Shadow_Logs;Uid=root;Pwd=123456;CharSet=utf8;"
      },
      "SqlServer": {
        "PoJun_Shadow": "USER ID=sa;PASSWORD=sa;INITIAL CATALOG=PoJun_Shadow;DATA SOURCE=127.0.0.1;CONNECT TIMEOUT=30",
        "PoJun_Shadow_Logs": "USER ID=sa;PASSWORD=sa;INITIAL CATALOG=PoJun_Shadow_Logs;DATA SOURCE=127.0.0.1;CONNECT TIMEOUT=30"
      },
      "MongoDB": {
        "PoJun_Shadow": "mongodb://127.0.0.1:27017/?slaveOk=true",
        "PoJun_Shadow_Logs": "mongodb://127.0.0.1:27017/?slaveOk=true"
      }
    },
    "RedisConfig": {
      "RedisExchangeHosts": "127.0.0.1,allowadmin=true",
      "RedisKey": "PoJun.Shadow.WebApi-dev",
      "RedisDB": 8
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
